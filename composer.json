{
    "name": "zfcampus/zf-problem-detail",
    "description": "PSR7 implementation for RFC 7807 Problem Details for HTTP APIs",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zend",
        "middleware",
        "rest",
        "problem-details"
    ],
    "homepage": "http://apigility.org/",
    "support": {
        "email": "apigility-users@zend.com",
        "irc": "irc://irc.freenode.net/apigility",
        "source": "https://github.com/zfcampus/zf-api-problem",
        "issues": "https://github.com/zfcampus/zf-api-problem/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.0-dev"
        },
        "zf": {
            "config-provider": "ZF\\ProblemDetails\\ConfigProvider"
        }
    },
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "http-interop/http-middleware": "^0.4.1",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^1.4",
        "zendframework/zend-eventmanager": "^3.0.1",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-view": "^2.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "ZF\\ProblemDetails\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\ProblemDetails\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
